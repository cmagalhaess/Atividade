import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    
    Scanner sc = new Scanner(System.in);
    int n, i, fat = 1;
    
    System.out.print("Informe um numero: ");
    n = sc.nextInt();
    
    for (i = 1; i <= n; i++){
      fat *= i;     
    }
    
    System.out.println("!" + n + " = " + fat);
  
    sc.close();
  }
}
