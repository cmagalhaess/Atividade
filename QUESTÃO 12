import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int pont = 0;
    
    System.out.println("Informe um numero: ");
    int x = sc.nextInt();

    for(int i = 0; i < x; i++){
      if(i%2!=0){
        System.out.println(" "+ i);
      }
    }
    sc.close();
  }
}
