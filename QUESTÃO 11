import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    
    Scanner sc = new Scanner(System.in);
    double pont = 0;
    
    System.out.print("Informe um numero: ");
    double x = sc.nextInt();
    System.out.print("Informe um numero: ");
    double y = sc.nextInt();

    for(int i = 0; i < y; i++){
      pont = (Math.pow(x, y));
    }
    System.out.println("A potencia dos números informados é igual a: "+ pont);
  }
}
